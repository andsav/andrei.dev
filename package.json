{
  "name": "dotcom",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "lint": "prettier ./src/**/*.vue ./src/**/*.js --write",
    "dev": "vite"
  },
  "dependencies": {
    "vue": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "^7.9.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "vite": "^1.0.0-rc.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "prettier ./src/**/*.vue ./src/**/*.js --write",
      "git add"
    ]
  }
}
