{
  "name": "dotcom",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "lint": "prettier ./src/**/*.vue ./src/**/*.js --write",
    "dev": "vite"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tachyons": "^4.12.0",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.11",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "^7.19.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "vite": "^1.0.0-rc.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "prettier ./src/**/*.vue ./src/**/*.js --write",
      "git add"
    ]
  }
}
