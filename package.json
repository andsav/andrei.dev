{
  "name": "dotcom",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "lint": "prettier ./src/**/*.vue ./src/**/*.js --write",
    "dev": "vite"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "tachyons": "^4.12.0",
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "^7.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "vite": "^1.0.0-rc.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "prettier ./src/**/*.vue ./src/**/*.js --write",
      "git add"
    ]
  }
}
